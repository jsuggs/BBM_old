   sr_tracker._setDomainName('none'); 
   sr_tracker._setAllowLinker(true); 
   sr_tracker._setAllowHash(false);
   sr_tracker._setCustomVar(1,"site","br",3);
   sr_tracker._trackPageview();
} catch(err) {}</script>
 



